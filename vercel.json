{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/fonts/(.*)",
      "dest": "/public/fonts/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Content-Type": "font/woff2"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}