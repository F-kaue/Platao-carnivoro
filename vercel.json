{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/beehiiv-subscribe.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/site-settings.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/navigation.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/content-blocks.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/newsletter-benefits.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/public-links.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/beehiiv-subscribe",
      "destination": "/api/beehiiv-subscribe.js"
    },
    {
      "source": "/api/site-settings",
      "destination": "/api/site-settings.js"
    },
    {
      "source": "/api/navigation",
      "destination": "/api/navigation.js"
    },
    {
      "source": "/api/content-blocks",
      "destination": "/api/content-blocks.js"
    },
    {
      "source": "/api/newsletter-benefits",
      "destination": "/api/newsletter-benefits.js"
    },
    {
      "source": "/api/public-links",
      "destination": "/api/public-links.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/fonts/(.*\\.woff2)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "font/woff2"
        }
      ]
    },
    {
      "source": "/fonts/(.*\\.woff)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "font/woff"
        }
      ]
    },
    {
      "source": "/fonts/(.*\\.ttf)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "font/ttf"
        }
      ]
    }
  ]
}